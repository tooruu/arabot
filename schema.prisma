datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = -1
}

model Prefix {
  guild_id BigInt @id @map("_id")
  prefix   String
}

model RrKick {
  guild_id     BigInt  @id @map("_id")
  kick_enabled Boolean
}
